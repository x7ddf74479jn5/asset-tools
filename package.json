  "devDependencies": {
    "@squoosh/cli": "0.7.2",
    "@svgr/cli": "6.2.1",
    "@types/node": "17.0.43",
    "@types/react": "17.0.39",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "budoux": "0.1.0",
    "dayjs": "1.11.3",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
