  "devDependencies": {
    "@squoosh/cli": "0.7.2",
    "@svgr/cli": "6.2.1",
    "@types/node": "17.0.16",
    "@types/react": "17.0.39",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "budoux": "0.0.3",
    "dayjs": "1.10.8",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "esmo": "0.14.1",
    "ora": "6.1.0",
    "prettier": "2.5.1",
    "typescript": "4.5.5",
    "zx": "5.2.0"
  },
  "scripts": {
    "type-check": "tsc -p tsconfig.json",
    "squoosh-optimize": "esmo src/squoosh.mts --mode optimize",
    "squoosh-resize": "esmo src/squoosh.mts --mode resize",
    "svgr": "esmo src/svgr.mts",
    "ffmpeg-convert": "esmo src/ffmpeg.mts --mode convert",
    "ffmpeg-resize": "esmo src/ffmpeg.mts --mode resize",
    "ffmpeg-clip": "esmo src/ffmpeg.mts --mode clip",
    "ffmpeg-extract-audio": "esmo src/ffmpeg.mts --mode extract-audio",
    "ffmpeg-twitter": "esmo src/ffmpeg.mts --mode twitter",
    "ffmpeg-gif": "esmo src/ffmpeg.mts --mode gif",
    "budoux": "esmo src/budoux.mts"
  }
}
