    "prettier": "2.7.0",
    "tsx": "3.4.2",
    "typescript": "4.7.3",
    "zx": "7.0.0"
  },
  "scripts": {
    "type-check": "tsc -p tsconfig.json",
    "squoosh-optimize": "tsx src/squoosh/index.mts --mode optimize",
    "squoosh-resize": "tsx src/squoosh/index.mts --mode resize",
    "svgr": "tsx src/svgr/index.mts",
    "ffmpeg-convert": "tsx src/ffmpeg/index.mts --mode convert",
    "ffmpeg-resize": "tsx src/ffmpeg/index.mts --mode resize",
    "ffmpeg-clip": "tsx src/ffmpeg/index.mts --mode clip",
    "ffmpeg-extract-audio": "tsx src/ffmpeg/index.mts --mode extract-audio",
    "ffmpeg-twitter": "tsx src/ffmpeg/index.mts --mode twitter",
    "ffmpeg-gif": "tsx src/ffmpeg/index.mts --mode gif",
    "budoux": "tsx src/budoux/index.mts"
  }
}
